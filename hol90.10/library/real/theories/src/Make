$hol90 -u rhol < nets.sml
$hol90 -u rhol < seq.sml
$hol90 -u rhol < lim.sml
$hol90 -u rhol < powser.sml
$hol90 -u rhol < transc.sml
/bin/rm -f ./rhol*
