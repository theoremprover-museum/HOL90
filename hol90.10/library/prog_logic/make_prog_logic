#!/bin/sh

if test -z "$THEORY_TYPE"; then THEORY_TYPE=ascii; fi

mkdir theories/$THEORY_TYPE 2>/dev/null || true
#echo 'use_and_exit print_HOL_ERR "theories/src/mk_lib_pc_logic.sml";' | $* &&
#echo 'use_and_exit print_HOL_ERR "theories/src/mk_lib_tc_logic.sml";' | $* &&
#echo 'use_and_exit print_HOL_ERR "theories/src/mk_lib_dijkstra.sml";' | $*

$* < theories/src/mk_lib_pc_logic.sml
$* < theories/src/mk_lib_tc_logic.sml
$* < theories/src/mk_lib_dijkstra.sml

